        <BesluitCompact xmlns="https://standaarden.overheid.nl/stop/imop/tekst/">
            <RegelingOpschrift eId="longTitle" wId="longTitle">
                <Al>Vaststelling van Omgevingsplan Gemeentestad (Zaltbommel)</Al>
            </RegelingOpschrift>
            <Aanhef eId="formula_1" wId="formula_1">
                <Al>De raad van de gemeente Gemeenstestad </Al>
                <Al>gelezen het voorstel van burgemeester en wethouders van 26 juni 2019, nr. 2019-4542;</Al>
                <Al>Gelet op <ExtRef ref="https://wetten.overheid.nl/jci1.3:c:BWBR0005416&amp;titeldeel=III&amp;hoofdstuk=IX&amp;artikel=149" soort="URL">Gemeentewet, art. 149</ExtRef>.</Al>
                <Al>besluit:</Al>
            </Aanhef>
            <Lichaam eId="body" wId="body">
                <WijzigArtikel eId="art_I" wId="gm0297_1__art_I">
                    <Kop>
                        <Label>Artikel</Label>
                        <Nummer>I</Nummer>
                    </Kop>
                    <Wat>Het omgevingsplan Gemeentestad is niet meer geldig.</Wat>
                </WijzigArtikel>
                <Artikel eId="art_II" wId="gm0297_1__art_II">
                    <Kop>
                        <Label>Artikel</Label>
                        <Nummer>II</Nummer>
                    </Kop>
                    <Inhoud>
                        <Al>Dit besluit treedt in werking per 14 november 2021.</Al>
                    </Inhoud>
                </Artikel>
            </Lichaam>
            <Sluiting eId="formula_2" wId="formula_2">
                <Al>Gegeven te Kaap Doorn, Gemeentestad, 3 december 2019</Al>
                <Ondertekening>
                    <Al>Gedeputeerde Raad van Apeldoorn tot Den Haag</Al>
                    <Al>Yue Wan Fan</Al>
                    <Al>gemeentesecretaris</Al>
                    <Al>Nadine Bauer</Al>
                    <Al>Burgemeester</Al>
                </Ondertekening>
            </Sluiting>
        </BesluitCompact>
