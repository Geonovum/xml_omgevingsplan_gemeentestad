    <WijzigArtikel eId="art_I" wId="gm0297_1__art_I">
     <Kop>
      <Label>Artikel</Label>
      <Nummer>I</Nummer>
     </Kop>
     <Wat>Gemeentestad ontwerp opgenomen in <IntRef ref="cmp_A" scope="WijzigBijlage">Bijlage A</IntRef> worden vastgesteld.</Wat>
    </WijzigArtikel>
    <Artikel eId="art_II" wId="gm0297_1__art_II">
     <Kop>
      <Label>Artikel</Label>
      <Nummer>II</Nummer>
     </Kop>
     <Inhoud>
